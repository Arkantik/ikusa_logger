{
  "applicationId": "ikusa.logger",
  "version": "1.8.5",
  "defaultMode": "window",
  "documentRoot": "/client/build/",
  "url": "/index.html",
  "enableServer": true,
  "enableNativeAPI": true,
  "nativeAllowList": [
    "app.exit",
    "filesystem.readFile",
    "filesystem.writeFile",
    "filesystem.writeBinaryFile",
    "os.open",
    "os.showOpenDialog",
    "os.showSaveDialog",
    "os.spawnProcess",
    "os.updateSpawnedProcess",
    "os.execCommand",
    "clipboard.writeText",
    "storage.setData",
    "storage.getData"
  ],
  "modes": {
    "window": {
      "title": "BDO Combat Logger",
      "width": 600,
      "height": 400,
      "minWidth": 600,
      "minHeight": 400,
      "maxWidth": 600,
      "maxHeight": 400,
      "icon": "/client/public/logo.png",
      "exitProcessOnClose": true,
      "enableInspector": false
    }
  },
  "cli": {
    "binaryName": "ikusa-logger",
    "resourcesPath": "/client/build/",
    "extensionsPath": "/extensions/",
    "binaryVersion": "5.6.0",
    "clientVersion": "5.6.0",
    "frontendLibrary": {
      "patchFile": "/client/index.html",
      "devUrl": "http://localhost:5173",
      "projectPath": "/client/",
      "initCommand": "npm install",
      "devCommand": "npm run dev",
      "buildCommand": "npm run build"
    }
  }
}

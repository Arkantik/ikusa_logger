{
	"applicationId": "ikusa.logger",
	"version": "1.6.0",
	"defaultMode": "window",
	"documentRoot": "/ui/build/",
	"url": "/",
	"enableServer": true,
	"enableNativeAPI": true,
	"nativeAllowList": [
		"app.exit",
		"filesystem.readFile",
		"filesystem.writeFile",
		"filesystem.writeBinaryFile",
		"os.open",
		"os.showOpenDialog",
		"os.showSaveDialog",
		"os.spawnProcess",
		"os.updateSpawnedProcess",
		"os.execCommand",
		"clipboard.writeText",
		"storage.setData",
		"storage.getData"
	],
	"modes": {
		"window": {
			"title": "Ikusa Logger",
			"width": 600,
			"height": 400,
			"minWidth": 600,
			"minHeight": 400,
			"maxWidth": 600,
			"maxHeight": 400,
			"icon": "/ui/static/logo.png",
			"exitProcessOnClose": false,
			"enableInspector": false
		}
	},
	"cli": {
		"binaryName": "ikusa-logger",
		"resourcesPath": "/ui/build/",
		"extensionsPath": "/extensions/",
		"binaryVersion": "4.10.0",
		"clientVersion": "3.8.2",
		"frontendLibrary": {
			"patchFile": "/ui/src/app.html",
			"devUrl": "http://localhost:5173"
		}
	}
}
